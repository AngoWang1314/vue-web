{
  "name": "vue-web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "unit": "jest --config jest.config.js --watchAll --coverage",
    "lint": "vue-cli-service lint",
    "build-storybook": "build-storybook -o build/storybook",
    "start-storybook": "start-storybook -p 9000"
  },
  "dependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@vue/babel-preset-app": "^4.5.9",
    "ant-design-vue": "^1.6.5",
    "axios": "^0.20.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-vue": "^2.0.2",
    "core-js": "^3.6.5",
    "identity-obj-proxy": "^3.0.0",
    "import-html-entry": "^1.9.0",
    "jest-transform-stub": "^2.0.0",
    "mobx": "^5.15.6",
    "mobx-vue": "^2.0.10",
    "prop-types": "^15.7.2",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.3",
    "vue-router": "^3.4.3",
    "vue-template-compiler": "^2.6.12",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^6.0.20",
    "@storybook/addons": "^6.0.20",
    "@storybook/vue": "^6.0.20",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.4",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.0.3",
    "antd-theme-webpack-plugin": "^1.3.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "compression-webpack-plugin": "^6.1.1",
    "eslint": "^6.7.2",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.6.3",
    "less": "^3.12.2",
    "less-loader": "^7.0.0",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "comma-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "semi": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-spaced-func": 2,
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "space-before-blocks": [
        2,
        "always"
      ],
      "default-case": 2
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      },
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      },
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
